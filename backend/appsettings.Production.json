{
  "ConnectionStrings": {
    "DefaultConnection": "__PRODUCTION_CONNECTION_STRING__"
  },
  "JWT": {
    "Key": "__PRODUCTION_JWT_KEY__",
    "Issuer": "WebTestUI",
    "Audience": "WebTestUIClient",
    "DurationInMinutes": 60
  },
  "AllowedOrigins": [
    "https://your-vercel-app-url.vercel.app"
  ],
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "SmtpPort": 587,
    "SenderEmail": "__PRODUCTION_EMAIL__",
    "SenderName": "API Testing Tool",
    "Password": "__PRODUCTION_EMAIL_PASSWORD__",
    "EnableSsl": true,
    "UseDefaultCredentials": false
  },
  "FileStorage": {
    "ProfileImagesPath": "wwwroot/uploads/profiles",
    "MaxFileSizeBytes": 2097152
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "K6": {
    "ExecutablePath": "k6"
  }
}
